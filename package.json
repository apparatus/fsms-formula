{
  "name": "service-name",
  "version": "0.0.1",
  "description": "fullstack-service description",
  "scripts": {
    "start": "node srv",
    "build-cmp": "cd cmp && npm run build",
    "test": "tap -R spec srv/test cmp/test --branches=90 --statements=90 --functions=90 --lines=90",
    "cov-report": "npm test -- --coverage-report=html",
    "install": "cd srv && npm install && cd ../cmp && npm install",
    "lint": "echo 'linting...' && spacey-standard",
    "audit": "cd srv && auditjs && cd ../cmp && auditjs"
  },
  "devDependencies": {
    "auditjs": "^1.2.0",
    "pre-commit": "^1.1.3",
    "spacey-standard": "^4.0.0",
    "tap": "^7.1.2"
  },
  "precommit": [
    "test",
    "lint"
  ],
  "license": "UNLICENSED"
}
